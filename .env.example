PX_TOPSTEP_FIRM=topstep
PX_TOPSTEP_APIKEY=
PX_TOPSTEP_USERNAME=

# the path to the docker db instance
DATABASE_URL=127.0.0.1:5432:5432

# Auto-update history settings
# Master switch
AUTO_UPDATE_ENABLED=true
# Comma-separated topics to cycle per instrument (order matters)
AUTO_UPDATE_TOPICS=Candles1s,Candles1m,Candles1h,Candles1d
# Optional substring filter to limit instruments during testing
AUTO_UPDATE_INSTRUMENT_FILTER=
# Preferred firm for PX providers (also set PX_*_FIRM above)
AUTO_UPDATE_PX_FIRM=topstep



# Max number of instruments to update concurrently per provider
AUTO_UPDATE_PER_PROVIDER_CONCURRENCY=10
